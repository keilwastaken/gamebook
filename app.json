{
  "expo": {
    "name": "gamebook",
    "slug": "gamebook",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/tab-bar-background.png",
    "scheme": "gamebook",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/tab-bar-background.png",
        "backgroundImage": "./assets/images/tab-bar-background.png",
        "monochromeImage": "./assets/images/tab-bar-background.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/tab-bar-background.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": false
    }
  }
}
